<h1>Chung's to do list</h1>

<form class="" action="/" method="post">

  <input type="text" name="todo" value="">
  <input type="submit" name="submit" value="Submit">

</form>


<table>
{{#each row}}
  <tr>
    <th>
      {{time}} :
    </th>
    <td>
      {{todo}}
    </td>
  </tr>
{{/each}}
</table>